	<Shell id="shell">
		<App id="app">
			<f:FlexibleColumnLayout id="flexibleColumnLayout" backgroundDesign="Translucent" layout="OneColumn" stateChange="onStateChanged">
				<f:beginColumnPages>
					<mvc:XMLView id="beginView" viewName="supplier.SupplierApp.view.Supplier"/>
				</f:beginColumnPages>
				<f:midColumnPages>
					<mvc:XMLView id="detailView" viewName="supplier.SupplierApp.view.SupplierDetail"/>
				</f:midColumnPages>
			</f:FlexibleColumnLayout>
		</App>
	</Shell>
</mvc:View>